@model string
@{
    ViewBag.Title = "GetJson";
}

@*<p>Value: @Model</p>*@
@*<input type="hidden" value="@Model" id="data"/>*@
<input type="hidden" value="@Model" id="data"/>

<script type="text/javascript" src="http://maps.google.com/maps/api/js?key=AIzaSyCfkpLOeJTDjRKcMzrmgZSUJMpZB0VK480"></script>
    <div id="mapz" style="width: 80%; height: 500px"></div>
<p>Value: @Model</p>
        <script>
            var data = document.getElementById("data").value;
            google.maps.event.addDomListener(window, 'load', initMap);
  function initMap() {
      var uluru = { lat: 31.311337, lng: 34.622824};
    var map = new google.maps.Map(document.getElementById('mapz'), {
                zoom: 12,
      center: uluru
            });
            map.data.addGeoJson(data);
            var featureStyle = {
                strokeColor: '#ff3333',
                strokeWeight: 4
            }

            map.data.setStyle(featureStyle);
    var marker = new google.maps.Marker({
                position: uluru,
      map: map
    });
  }
</script>

